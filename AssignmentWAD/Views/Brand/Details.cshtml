@model dynamic

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/Admin_Layout.cshtml";
    AssignmentWAD.Models.Brand brand = Model.Brand;
}

<h2>Details</h2>

<div>
    <h4>Brand</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Name
        </dt>

        <dd>
            @brand.Name
        </dd>

        <dt>
            Image
        </dt>

        <dd>
            @brand.Image
        </dd>

        <dt>
            Description
        </dt>

        <dd>
            @brand.Description
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = brand.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
